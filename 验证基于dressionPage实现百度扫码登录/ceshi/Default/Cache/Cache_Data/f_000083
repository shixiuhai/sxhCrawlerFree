                    var link = document.location.protocol + '//nsclick.baidu.com/v.gif?'
                    + 'pid=111&data_source=fe&type=1023'
                    + '&source=' + (publicData.clientfrom || 'wap')
                    + '&client=' + (publicData.client || '')
