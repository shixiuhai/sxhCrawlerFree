            function insertScript(jsFile, callback) {
                window.jsFileList = window.jsFileList || {};
                if (!window.jsFileList[jsFile]) {
                    window.jsFileList[jsFile] = true;
                    var d = document;
                    var s = d.createElement('script');
                    s.type = 'text/javascript';
                    s.charset = 'utf-8';
                    if (s.readyState) {
                        s.onreadystatechange = function () {
                            if (s.readyState === 'loaded' || s.readyState === 'complete') {
                                s.onreadystatechange = null;
                                callback && callback();
                            }
                        };
                    } else {
                        s.onload = function () {
                            callback && callback();
                        };
                    }
                    s.src = jsFile;
                    d.getElementsByTagName('head')[0].appendChild(s);
                } else {
                    callback && callback();
                }
            }
            // 端外更新TBDUSS
            try {
                var browserUserAgent = window && window.navigator && window.navigator.userAgent;
                if (!/bdoa_/.test(browserUserAgent) && !/Sapi_/.test(browserUserAgent)) {
                    insertScript('https://wappass.baidu.com/static/waplib/passRefreshTk.js?tt='
                    + new Date().getTime(), function () {
                        var refreshTkInit = (window.passRefreshTk && window.passRefreshTk.refreshTkInit) || '';
