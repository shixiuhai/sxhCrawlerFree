                        link += '&auto_statistic=' + btoa('{eventType:security-js-error}');
                        link += '&extrajson=' + (e.filename || '') + ' ' + (e.lineno || '');
                        link += '&subpro=' + (e.message || '');
                    } else {
                    // 文件加载错误
                        link += '&auto_statistic=' + btoa('{eventType:security-load-error}');
                        link += '&extrajson=' + (e.target.src || '');
                        link += '&subpro=' + (e.target.localName || '') + (e.type || '');
                    }
                    var img = new Image();
                    img.onload = img.onerror = function () {
                        img.onload = img.onerror = null;
                        img = null;
                    };
                    img.src = link;
                } catch(e) {
                    // catch
                };
            },true);
